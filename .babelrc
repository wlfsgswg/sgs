{
    "presets": [
        [
            "@babel/preset-env",
            {
                "useBuiltIns": "usage",
                "corejs": 3
            }
        ],
        [
            "@babel/preset-react",
        ]
    ],
    // antd-mobile按需加载引入css需要下载babel-plugin-import
    "plugins": [
        [
            "@babel/plugin-proposal-class-properties",
            {
                "loose": true
            }
        ],
        ["import", { "libraryName": "antd-mobile", "style": "css" }] // `style: true` 会加载 less 文件
    ]
}